<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>PassportPDF</AssemblyName>
    <PackageId>PassportPDF</PackageId>
    <OutputType>Library</OutputType>
    <Authors>PassportPDF</Authors>
    <Company>ORPALIS</Company>
    <AssemblyTitle>PassportPDF</AssemblyTitle>
    <Description>ORPALIS PassportPDF API Client SDK</Description>
    <Copyright>Copyright Â® 2018 - 2019 ORPALIS - http://www.orpalis.com</Copyright>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <RootNamespace>PassportPDF</RootNamespace>
    <Version>1.0.43</Version>
    <AssemblyVersion>1.0.43</AssemblyVersion>
    <FileVersion>1.0.43</FileVersion>
    <PackageIconUrl>http://www.orpalis.com/images/ORPALIS/logos/PP-symbol-128.png</PackageIconUrl>
    <PackageProjectUrl>https://passportpdf.com/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Orpalis/passportpdfsdk-net</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.6.9" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="JsonSubTypes" Version="1.5.2" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
  </ItemGroup>

</Project>